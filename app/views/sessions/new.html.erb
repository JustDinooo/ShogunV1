<h1>Sign in</h1>

<%= form_with url: sign_in_path do |form| %>
	<div class="form-group">
		<%= form.label :"Email" %>
		<%= form.text_field :email, class: "form-control" %>
	</div>
	
	<div class="form-group">
		<%= form.label :"password" %>
		<%= form.password_field :password, class: "form-control" %>
	</div>
	
	<br/>
	<div class="actions">
    	<%= form.submit "Sign in", class:"btn btn-secondary" %>
  	</div>
<% end %>